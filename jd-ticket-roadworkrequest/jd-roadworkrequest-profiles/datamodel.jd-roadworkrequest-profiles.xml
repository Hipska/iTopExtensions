          <class id="RoadworkRequest"/>
		  <class id="ContactMethod"/>
		  <class id="AuthenticationMethod"/>
		  <class id="Attachment"/>
        </classes>
      </group>
		
		<group id="PrecannedReplies" _delta="define_if_not_exists">
