<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.4">

  <classes>
     
	
    <class id="LendRecord" _delta="define">
      <parent>cmdbAbstractObject</parent>
      <properties>
        <category>bizmodel,searchable</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>lendrecord</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field/>
		
		<naming>
			<attributes>
				<attribute id="name"/>
			</attributes>
		</naming>
 
        <display_template/>
        <icon>images/lendrecord.png</icon>
        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="id"/>
            <attribute id="org_id"/>
            <attribute id="organization_name"/>
          </attributes>
        </reconciliation>
      </properties>
	  
	  
      <fields>
		<field id="org_id" xsi:type="AttributeExternalKey">
			<sql>org_id</sql>
			<target_class>Organization</target_class>
			<is_null_allowed>false</is_null_allowed>
			<on_target_delete>DEL_AUTO</on_target_delete>
		</field>	
				
		<field id="team_id" xsi:type="AttributeExternalKey">
			<filter><![CDATA[SELECT Team ]]></filter>
			<sql>team_id</sql>
			<target_class>Team</target_class>
			<is_null_allowed>false</is_null_allowed>
			<on_target_delete>DEL_MANUAL</on_target_delete>
		</field>
		<field id="team_name" xsi:type="AttributeExternalField">
			<extkey_attcode>team_id</extkey_attcode>
			<target_attcode>email</target_attcode>
		</field>
  
        <field id="date_in" xsi:type="AttributeDate">
          <sql>date_in</sql>
		  <is_null_allowed>false</is_null_allowed>  
        </field>
		
        <field id="date_out" xsi:type="AttributeDate">
          <sql>date_out</sql>
		  <is_null_allowed>false</is_null_allowed>  
        </field>
		
        <field id="type" xsi:type="AttributeEnum"> 
          <sql>type</sql>  
		  <values>
			<value id="on_site">on_site</value>
			<value id="remote">remote</value>
		  </values>
		  <default_value>on_site</default_value>
          <is_null_allowed>false</is_null_allowed>
		  <display_style>radio</display_style>		  
        </field>
		 
        <field id="remarks" xsi:type="AttributeCaseLog"> 
          <sql>remarks</sql>  
		  <default_value/>
          <is_null_allowed>true</is_null_allowed> 
        </field>
		
		<field id="intern_id" xsi:type="AttributeString">
          <sql>intern_id</sql>
		<default_value/>
		<is_null_allowed>true</is_null_allowed>
		<display_style>list</display_style>
		</field>
		
		<field id="contact_id" xsi:type="AttributeExternalKey">
					<sql>contact_id</sql>
					<target_class>Contact</target_class>
					<is_null_allowed>false</is_null_allowed>
					<on_target_delete>DEL_AUTO</on_target_delete>
		</field>
			
      </fields> 
      <presentation>
        <details>
